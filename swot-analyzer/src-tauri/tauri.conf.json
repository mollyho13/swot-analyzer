{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "SWOT Analyzer",
  "version": "0.1.0",
  "identifier": "com.swot.analyzer",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "SWOT Analyzer",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true
      }
    ]
  }
}